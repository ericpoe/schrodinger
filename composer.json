{
    "name": "ericpoe/schrodinger",
    "description": "A Schrodinger is a trinary-state object that has a state of BOTH until directly observed.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Poe",
            "email": "eric.poe@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
    "platform": {
        "php": "7.4.0"
    },
    "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Schrodinger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schrodinger\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyze": "vendor/bin/phpstan analyse",
        "lint": "vendor/bin/phpcs",
        "lint-fix": "vendor/bin/phpcbf"
    }
}
